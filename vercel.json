{
  "buildCommand": "cd scrap-metal-webapp && npm run build",
  "outputDirectory": "scrap-metal-webapp/.next",
  "installCommand": "cd scrap-metal-webapp && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/scrap-metal-webapp/$1"
    }
  ]
} 